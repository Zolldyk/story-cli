{
  "name": "story-cli",
  "version": "1.0.0",
  "description": "CLI tool for Story Protocol - Register and manage IP assets on the blockchain",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "story": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "tsx src/index.ts",
    "lint": "eslint src/",
    "format": "prettier --write 'src/**/*.ts'",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zolldyk/story-cli.git"
  },
  "keywords": [
    "story-protocol",
    "ip",
    "blockchain",
    "cli"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Zolldyk/story-cli/issues"
  },
  "homepage": "https://github.com/Zolldyk/story-cli#readme",
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0"
  }
}
