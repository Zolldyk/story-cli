{
  "name": "story-cli",
  "version": "0.1.0",
  "description": "CLI tool for Story Protocol - Register and manage IP assets on the blockchain",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "story": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "tsx src/index.ts",
    "lint": "eslint src/",
    "format": "prettier --write 'src/**/*.ts'",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:docker": "docker compose -f docker-compose.test.yml up --build --abort-on-container-exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zolldyk/story-cli.git"
  },
  "keywords": [
    "story-protocol",
    "ip",
    "intellectual-property",
    "blockchain",
    "cli",
    "ipfs",
    "nft",
    "web3"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Zolldyk/story-cli/issues"
  },
  "homepage": "https://github.com/Zolldyk/story-cli#readme",
  "devDependencies": {
    "@types/didyoumean": "^1.2.3",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^1.6.1",
    "eslint": "^8.56.0",
    "prettier": "^3.2.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "dependencies": {
    "@pinata/sdk": "^2.1.0",
    "@story-protocol/core-sdk": "1.4.2",
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "didyoumean": "^1.2.2",
    "inquirer": "^9.2.12",
    "mermaid": "^10.6.1",
    "node-fetch": "^3.3.2",
    "ora": "^7.0.1",
    "viem": "2.21.0"
  }
}
